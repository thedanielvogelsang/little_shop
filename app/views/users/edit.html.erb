<body>
  <div>
    <div class="container">
      <h3>Create New Account:</h3>
      <%= form_for @user do |f| %>
      <%= f.label :first_name %>
      <%= f.text_field :first_name%><br>
      <%= f.label :last_name %>
      <%= f.text_field :last_name %><br>
      <%= f.label :username %>
      <%= f.text_field :username %><br>
      <%= f.label :password %>
      <%= f.text_field :password %><br>
      <%= f.label :email %>
      <%= f.text_field :email %><br>
      <%= f.submit "Update Account" %>
      <% end %>
    </div>
  </div>
</body>
